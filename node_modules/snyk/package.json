    "es6-promise": "^3.0.2",
    "inquirer": "1.0.3",
    "lodash": "^3.10.1",
    "node-uuid": "^1.4.3",
    "open": "^0.0.5",
    "os-name": "^1.0.3",
    "request": "^2.74.0",
    "semver": "^5.1.0",
    "snyk-config": "1.0.1",
    "snyk-module": "1.7.0",
    "snyk-policy": "1.5.2",
    "snyk-recursive-readdir": "^2.0.0",
    "snyk-resolve": "1.0.0",
    "snyk-resolve-deps": "1.7.0",
    "snyk-tree": "^1.0.0",
    "snyk-try-require": "^1.0.0",
    "tempfile": "^1.1.1",
    "then-fs": "^2.0.0",
