{
  "name": "enchantedmirror",
  "version": "1.0.0",
  "description": "A modular interface for smart mirrors.",
  "main": "js/electron.js",
  "scripts": {
    "start": "electron js/electron.js",
    "jscs": "jscs **/**/**/**/*.js",
    "stylelint": "stylelint css/main.css fonts/roboto.css",
    "htmlvalidator": "html-validator --file=index.html",
    "test": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcmillon/EnchantedMirror.git"
  },
  "keywords": [
    "magic mirror",
    "smart mirror",
    "mirror UI",
    "modular"
  ],
  "devDependencies": {
    "electron-prebuilt": "latest",
    "grunt": "latest",
    "grunt-eslint": "latest",
    "grunt-postcss": "latest",
    "postcss-reporter": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest",
    "time-grunt": "latest"
  },
  "dependencies": {
    "express": "latest",
    "feedme": "latest",
    "iconv-lite": "latest",
    "moment": "latest",
    "request": "latest",
    "snyk": "latest",
    "socket.io": "latest",
    "valid-url": "latest",
    "walk": "latest",
    "rrule": "latest"
  }
}
